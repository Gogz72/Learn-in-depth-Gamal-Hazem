#include <stdio.h>

void main()
{
	float a [2][2];
	float b [2][2];

	float sum [4][4];

	int i , j;


	while(1)
	{




		printf("Enter the elements of the 1st matrix");

		fflush(stdin); fflush(stdout);

		for (i=0;i<2;i++)
		{
			for (j=0;j<2;j++)

			printf("Enter a%d%d",i,j);

			fflush(stdin); fflush(stdout);

			scanf("%f",a[i][j]);

	    }




}
